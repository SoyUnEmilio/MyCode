                                        <GradientStop Color="#FF606060" Offset="1"/>
                                        <GradientStop Color="#FF121212"/>
                                        <GradientStop Color="#FE212121" Offset="0.322"/>
                                    </LinearGradientBrush>
                                </Ellipse.Fill>
                            </Ellipse>
                        </Grid>
                        <GroupBox x:Name="Accounts" Style="{DynamicResource PerformTransferGroupBoxStyle1}" Foreground="White" Header="Accounts" Background="{DynamicResource GroupBoxBackgroundGradient}" BorderBrush="{x:Null}" FontFamily="Helvetica35" FontSize="18.667" Width="400" Height="300" Margin="0,10,0,0">
                            <Grid>
                                <ListView x:Name="LV_Accounts" Margin="0,42,0,32" ItemsSource="{Binding BankAccounts}" Style="{DynamicResource ListViewStyle1}" ItemContainerStyle="{DynamicResource CustomerListBoxItemStyle}" ItemTemplate="{DynamicResource BankAccountsDataTemplate}" IsSynchronizedWithCurrentItem="True" SelectedItem="{Binding BankAccountSelected}"/>
                                <Button x:Name="BTN_OK" Content="OK" VerticalAlignment="Bottom" FontSize="12" Style="{DynamicResource ButtonStyle}" Foreground="White" Margin="20,0,20,6" Height="20" Width="200" Command="{Binding SetSelectedAccountCommand}"/>
                                <ComboBox x:Name="CB_FilterSelector" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="0,8,0,0" FontSize="12" Style="{DynamicResource ComboBoxStyle}" SelectedIndex="{Binding FilterSelector, FallbackValue=1}">
                                    <ComboBoxItem Content="Bank Account Number" FontSize="12"/>
                                    <ComboBoxItem Content="Customer Name" FontSize="12" Selector.IsSelected="True"/>
                                </ComboBox>
                                <TextBox x:Name="TB_Filter" TextWrapping="Wrap" Text="{Binding FilterBankAccount, Mode=TwoWay}" VerticalAlignment="Top" Margin="124,8,69.534,0" Style="{DynamicResource TextBoxStyle1}" FontSize="12"/>
                                <Button x:Name="BTN_Search" Margin="0,0,8.204,0" Style="{DynamicResource SearchButton}" Content="Search" FontFamily="{DynamicResource SystemFont}" FontSize="12" Foreground="White" Command="{Binding SearchAccountCommand, Mode=OneWay}" Cursor="Hand" HorizontalAlignment="Right" Height="38.257" VerticalAlignment="Top"/>
                            </Grid>
                        </GroupBox>
                    </Grid>
                </Popup>
            </Grid>
        </Border>
    </Border>
</UserControl>